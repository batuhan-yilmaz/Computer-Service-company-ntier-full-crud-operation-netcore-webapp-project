                            <div class="col-md-6">
                                <h4>Email:</h4>
                                <p>bilgi@3escomputer.com</p>
                            </div>
                            <div class="col-md-6">
                                <h4>Phone No.</h4>
                                <p>(+90) 224 253 4966</p>
                            </div>
                        </div>
                        <form action="#" role="form">
                            <div class="input-group subscribe">
                                <input type="email" class="form-control" placeholder="Yeniliklerden haberdar olmak için abone olun! (Mail adresiniz)" required="">
                                <span class="input-group-addon">
                                    <button class="btn" type="submit"></button>
                                </span>
                            </div>
                        </form>
                    </div><!--/ end about us -->

                </div><!-- Row end -->
            </div><!-- Container end -->
