                        </button>

                                <partial name="_logo" />

                    </div><!--/ Logo end -->
                    <nav class="collapse navbar-collapse clearfix" role="navigation">
                        <ul class="nav navbar-nav navbar-right">
                            <li>
                                <a  asp-controller="Home" asp-action="Index" class=" navbar-link">Ana Sayfa <i class="fa fa-angle-down"></i></a>

                            </li>
                            <li>
                                <a  asp-controller="Home" asp-action="Index" asp-fragment="about" class="dropdown-toggle navbar-link @(ViewBag.menuactive=="Index"?"active":"")">Şirketimiz </a>

                            </li>

                            <li class="dropdown">
                                <a  asp-controller="Home" asp-action="Services" class="dropdown-toggle navbar-link @(ViewBag.menuactive=="Services"?"active":"")">Hizmetlerimiz <i class="fa fa-angle-down"></i></a>

                            </li>


                            <li class="dropdown">
                                <a asp-controller="Home" asp-action="Index" asp-fragment="team" class="dropdown-toggle navbar-link @(ViewBag.menuactive=="Index"?"active":"")">İş Ortaklarımız <i class="fa fa-angle-down"></i></a>

                            </li>
                            <li><a asp-controller="Home" asp-action="Index" asp-fragment="contact"  class="dropdown-toggle navbar-link @(ViewBag.menuactive=="Index"?"active":"")">İletişim</a></li>
                        </ul>
                    </nav><!--/ Navigation end -->
                </div><!--/ Row end -->
            </div><!--/ Container end -->
        </header><!--/ Header end -->
        <!-- Slider start -->


        @RenderBody()

        <!-- Footer start -->
        <footer id="footer" class="footer">
