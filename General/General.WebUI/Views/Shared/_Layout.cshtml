        <!-- Style Switcher -->
        <script type="text/javascript" src="~/js/style-switcher.js"></script>
        <!-- Owl Carousel -->
        <script type="text/javascript" src="~/js/owl.carousel.js"></script>
        <!-- PrettyPhoto -->
